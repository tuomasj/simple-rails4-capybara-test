<h1>Sessions#new</h1>
<% if flash[:error] %>
<p class="error"><%= flash[:error] %></p>
<% end %>
<%= form_tag sessions_path do %>
  <div class="row">
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>

  <div class="row">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class="row">
    <%= submit_tag "Login" %>
  </div>
<% end %>
